<?xml version="1.0" encoding="utf-8"?>
<project>
	<!-- NMML reference: https://gist.github.com/1763850 -->
	
	<!-- metadata, make sure 'package' is at least 3 segments (ie. com.mycompany.myproject) -->
	<meta title="Notification_test" package="test.josuigoa.notifications" version="1.0.0" company="Josu Igoa" />
	
	<!-- output -->
	<app main="Main" file="NotificationsTest" path="bin" />
	
	<window background="#FFFFFF" fps="60" />
	<window width="800" height="480" unless="mobile" />
	<window orientation="portrait" vsync="false" antialiasing="0" if="cpp" />
	
	<!-- classpath, haxe libs -->
	<source path="src" />
	<haxelib name="openfl" />
	<haxelib name="actuate" />
	<haxelib name="Notifications" />
	
	<!-- assets -->
	<icon path="assets/openfl.svg" />
	
	<!-- optimize output
	<haxeflag name="-dce full" /> -->
	
</project>